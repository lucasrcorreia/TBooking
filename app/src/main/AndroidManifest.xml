<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="rolu18oy.ju.se.layoutapp">

    <uses-permission android:name="android.permisssion.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Test"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@drawable/progress_animation"/>

        <activity
            android:name=".NavigationActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginNavActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreateAccNavActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".r_NavigationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".r_CreateAccountActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>